{
  "name": "form-container",
  "version": "0.1.4",
  "engines": {
    "node": ">=6.0.0"
  },
  "description":
    "Lightweight React form container with validation (written in TypeScript)",
  "keywords": ["react", "hoc", "form", "container", "validation", "typescript"],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --env=jsdom",
    "test:coverage": "npm run test -- --coverage",
    "test:dev": "npm run test -- --watch",
    "clean": "shx rm -rf _bundles lib lib-esm",
    "build": "rimraf dist && webpack --hide-modules && rimraf dist/ts-build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitkon/form-container.git"
  },
  "license": "MIT",
  "dependencies": {
    "hoist-non-react-statics": "^2.3.1",
    "lodash": "^4.17.4",
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "devDependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^21.1.6",
    "@types/lodash": "^4.14.85",
    "@types/react": "^16.0.22",
    "dts-bundle": "^0.7.3",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest": "^21.2.1",
    "raf": "^3.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.2.2",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.8.1"
  },
  "peerDependencies": {},
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "license": "MIT",
    "dependencies": {
        "hoist-non-react-statics": "^2.3.1",
        "lodash": "^4.17.4",
        "react": "^16.1.0",
        "react-dom": "^16.1.0"
    },
    "devDependencies": {
        "@types/enzyme-adapter-react-16": "^1.0.1",
        "@types/jest": "^22.0.0",
        "@types/lodash": "^4.14.85",
        "@types/react": "^16.0.22",
        "dts-bundle": "^0.7.3",
        "enzyme": "^3.2.0",
        "enzyme-adapter-react-16": "^1.1.0",
        "jest": "^22.0.0",
        "raf": "^3.4.0",
        "rimraf": "^2.6.2",
        "ts-jest": "^21.2.2",
        "ts-loader": "^3.1.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "webpack": "^3.8.1"
    },
    "setupFiles": ["raf/polyfill"],
    "setupTestFrameworkScriptFile": "./src/setupTests.ts"
  }
}
